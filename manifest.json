    "icons": {
       "128": "icon.png",
       "16": "icon.png",
       "48": "icon.png"
    },
    "manifest_version": 2,
    "name": "Hide Ranking Maker",
    "permissions": [ "tabs" ],
    "version": "0.1.0"
 }
 